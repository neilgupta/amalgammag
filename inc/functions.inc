<?phpfunction getCategory($cid) {	if ($cid == c1) {		return 'Stories';	}	elseif ($cid == c3) {		return 'Poems';	}	elseif ($cid == c4) {		return 'Jokes';	}	elseif ($cid == c5) {		return 'Art';	}	elseif ($cid == c7) {		return 'Academia';	}	elseif ($cid == c8) {		return 'Popular Debate';	}	elseif ($cid == c9) {		return 'Science & Tech';	}	elseif ($cid == c11) {		return 'Fashion';	}	elseif ($cid == c12) {		return 'Movies';	}	elseif ($cid == c13) {		return 'Music';	}	elseif ($cid == c15) {		return 'Experiences';	}	elseif ($cid == c19) {		return 'Editor\'s Notes';	}	elseif ($cid == c20) {		return 'Fear Essays';	}	elseif ($cid == c21) {		return 'Sports';	}	elseif ($cid == c22) {		return 'Poe Stories';	}	else {		return 'UNKNOWN';	}}function getIssue($issueid) {	$i = substr($issueid, 1);	return 'Amalgam: Issue '.$i;}?>